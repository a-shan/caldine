<div class="panel panel-default devise-bs">
	<div class="panel-body">
		<h3><%= @user.first_name %>'s Profile</h3>
		<p>Name: <%= @user.first_name %> <%= @user.last_name %></p>
		<p>Email: <%= @user.email %></p>
		<p>Phone: <%= @user.phone_number %></p>
        <p>About: <%= @user.description %></p>
		<%= link_to "Edit Profile", edit_user_path(@user), :method => :get %>
		<!--<%= link_to "Delete Account", @user, :method => :delete, data: { confirm: "Are you sure you want to delete your account?" } %>-->
	</div>
</div>
