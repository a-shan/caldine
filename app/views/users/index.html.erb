<h1>Welcome <%= @current_user.first_name %>!</h1>

<% @users.each do |user| %>
    <h1><%= user.first_name %></h1>
<% end %>


<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
  </tr>
  <% @users.each do |user| %> 
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
      </tr>
  <% end %>
</table>



