<div class="panel panel-default devise-bs">
<div align ="center">
<div class="container">
  <div class="row">
    <h3 align="center">Locations</h3>
    <div class="column">
      <table class="table">
        <tbody align ="center">
          <% @locations.each do |loc| %>
            <tr>
              <td><%= link_to loc.name, location_path(loc) %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
</div>
</div>